#include <stdio.h>

int main(void) {
    int t;
    scanf("%d",&t);
    for (int i=t;i>=1;i--){
        int n;
        scanf("%d",&n);
        if (n==1){
            printf("1 1");
            printf("\n");
        }
        else if (n%2==0){
            printf("%d ",(n/2));
            printf("%d",n);
            printf("\n");
        }
        else {
            printf("%d ",(n/2)+1);
            printf("%d",n);
            printf("\n");
        }
    }
    
}

